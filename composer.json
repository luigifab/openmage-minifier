{
    "type": "magento-module",
    "name": "luigifab/openmage-minifier",
    "description": "A module to minify CSS and JS with uglify-js and clean-css for OpenMage.",
    "keywords": ["openmage", "openmage-module"],
    "homepage": "https://github.com/luigifab/openmage-minifier",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Fabrice Creuzot (luigifab)",
            "homepage": "https://www.luigifab.fr",
            "email": "code@luigifab.fr"
        }
    ],
    "support": {
        "issues": "https://github.com/luigifab/openmage-minifier/issues",
        "docs": "https://www.luigifab.fr/openmage/minifier"
    },
    "require": {
        "php": ">=7.2.0",
        "openmage/magento-lts": ">=19.4.0 || main-dev || next-dev",
        "symfony/polyfill-php80": "*",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-mbstring": "*",
        "ext-zlib": "*"
    },
    "suggest": {
        "ext-tidy": "Needed to minify HTML code."
    },
    "minimum-stability": "dev",
